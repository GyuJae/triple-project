{
  "name": "triple-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:es": "eslint '**/*.{js,ts,tsx}'",
    "lint:es:fix": "npm run lint:es -- --fix",
    "lint:etc": "prettier '**/*.{json,yaml,md}' --check",
    "lint:etc:fix": "prettier '**/*.{json,yaml,md}' --write",
    "lint:style": "stylelint './**/*.{js,ts,tsx}'",
    "lint:style:fix": "stylelint './**/*.{js,ts,tsx}' --fix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "framer-motion": "^6.3.14",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-intersection-observer": "^9.3.4",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@titicaca/eslint-config-triple": "^3.1.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@vitejs/plugin-react": "^1.3.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "homepage": "https://gyujae.github.io/triple-project"
}
